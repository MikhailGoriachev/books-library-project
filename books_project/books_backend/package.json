   "scripts": {
      "build": "nest build",
      "start": "nest start",
      "start:dev": "nest start --watch",
      "start:debug": "nest start --debug --watch",
      "start:prod": "node dist/main",
      "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
      "test": "jest",
      "test:watch": "jest --watch",
      "test:cov": "jest --coverage",
      "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
      "test:e2e": "jest --config ./test/jest-e2e.json",
      "typeorm": "typeorm-ts-node-commonjs",
      "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed",
      "db:create": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create",
      "db:drop": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop"
   },
   "dependencies": {
      "@nestjs-modules/mailer": "^1.9.1",
      "@nestjs/common": "^9.0.0",
      "@nestjs/config": "^3.0.0",
      "@nestjs/core": "^9.0.0",
      "@nestjs/jwt": "^10.1.0",
      "@nestjs/passport": "^10.0.0",
      "@nestjs/platform-express": "^9.0.0",
      "@nestjs/serve-static": "^4.0.0",
      "@nestjs/typeorm": "^10.0.0",
      "@types/bcrypt": "^5.0.0",
      "@types/multer": "^1.4.7",
      "bcrypt": "^5.1.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "dotenv": "^16.3.1",
      "fs": "^0.0.1-security",
      "handlebars": "^4.7.8",
      "multer": "^1.4.5-lts.1",
      "mysql2": "^3.3.5",
      "nodemailer": "^6.9.5",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "passport-local": "^1.0.0",
      "path": "^0.12.7",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "rxjs": "^7.2.0",
      "transliteration": "^2.3.5",
      "typeorm": "0.3.16",
      "typeorm-extension": "^2.8.1",
      "uuid": "^9.0.1"
   },
   "devDependencies": {
      "@nestjs/cli": "^9.0.0",
      "@nestjs/schematics": "^9.0.0",
      "@nestjs/testing": "^9.0.0",
      "@types/express": "^4.17.13",
      "@types/jest": "29.5.1",
      "@types/node": "18.16.12",
      "@types/nodemailer": "^6.4.11",
      "@types/passport-jwt": "^3.0.8",
      "@types/passport-local": "^1.0.35",
      "@types/supertest": "^2.0.11",
      "@types/transliteration": "^1.6.6",
      "@types/uuid": "^9.0.4",
      "jest": "29.5.0",
      "source-map-support": "^0.5.20",
      "supertest": "^6.1.3",
      "ts-jest": "29.1.0",
      "ts-loader": "^9.2.3",
      "ts-node": "^10.0.0",
      "tsconfig-paths": "4.2.0",
      "typescript": "^5.0.0"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   }
}
